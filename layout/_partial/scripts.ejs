<% if (theme.gitalk.enable){ %><%- js(['//cdn.jsdelivr.net/npm/gitalk@1/dist/gitalk.min.js'])%><%}%>
<%- js(['//cdn.jsdelivr.net/npm/jquery-1.8@1.8.3/jquery.min.js', 'js/plugin.js', 'js/typed.js', 'js/diaspora.js'])%>
<%- partial('photoswipe') %>

<script type="text/javascript">
    $(document).ready(function() {
        if ($('.subscribed-user-container').length) {
            $.ajax({
                url: '/socialbinding/count/users',
                success: function(data) {
                    $('.subscribed-user.wechat-user').text(data['WECHAT']);
                }
            });
        }
    });
</script>
